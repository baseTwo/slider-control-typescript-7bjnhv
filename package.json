{
  "name": "slider",
  "version": "0.0.0",
  "author": "Paul den Boer",
  "private": true,
  "scripts": {
    "build": "npm run clean:stage && npm run tsc && npm run webpack",
    "watch": "concurrently -k \"tsc --pretty --watch\" \"webpack --progress --watch\"",
    "tsc": "tsc --pretty",
    "webpack": "webpack --progress",
    "webpack:prod": "webpack ./stage/tsc/index.js -o ./dist --progress --mode=production",
    "webpack:dev": "webpack ./stage/tsc/index.js -o ./dist --progress --mode=development",
    "serve": "webpack serve",
    "rimraf": "rimraf",
    "clean:stage": "npm run rimraf -- stage"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "concurrently": "^7.1.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
