    "rimraf": "rimraf",
    "clean:stage": "npm run rimraf -- stage"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
